{
  "name": "functions",
  "version": "1.0.0",
  "main": "build/index.js",
  "license": "MIT",
  "engines": {
    "node": "8"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "yarn run build --watch",
    "clean": "rm -rf build",
    "serve": "firebase serve --only functions --port 5001",
    "serve:offline": "yarn run serve --offline",
    "shell": "firebase functions:shell --port 5001",
    "logs": "firebase functions:log",
    "tslint": "tslint '{src,test}/**/*.ts{,x}'",
    "tslint:fix": "tslint --fix '{src,test}/**/*.ts{,x}'"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "firebase": "^7.5.0",
    "firebase-admin": "^8.8.0",
    "firebase-functions": "^3.3.0",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.14",
    "@types/lodash": "^4.14.149",
    "@types/node-fetch": "^2.5.4",
    "typescript": "^3.7.2"
  }
}
